{
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "env": {
    "node": true,
    "jest": true,
    "es2021": true 
  },
   "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
     "prettier/prettier": "error",
     "no-console": "off",
     "import/extensions": [
       "error",
       "always",
       {
         "js": "never"
       }
     ]
   }
 }